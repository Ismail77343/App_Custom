 "css": "@media print {\n    .page-break {\n        page-break-before: always;\n    }\n    .ffooter{\n        position:fixed;\n        bottom:0;\n    }\n}\np{\n    margin:0;\n    padding:0;\n    font-size:8pt;\n}\n.hidden-pdf{\n    display:none;\n}\n._c{\n    color:#2F75B5;\n    text-align:center;\n}\n.a_line{\n    border-top:1.0pt solid #9BC2E6;\n    border-bottom:1.0pt solid #9BC2E6;\n}\n.fsm{\n   font-size: 10.0pt;\n}\n.fb{\n    font-weight:700;\n}\n._header{\n    background:#2F75B5;\n    color:white !important;\n}\n.ltr{\ntext-align:left;\ndirection:ltr;\n}\n.rtl{\ntext-align:right;\ndirection:rtl;\n}\n.fsm2{\n       font-size: 11.0pt;\n}\n.tbl td{\n    padding:0 !important;\n}\n.tbl td{\n    padding:0 !important;\n}\n._ctbl{\n    background:#1F4E78;\n    color:white !important;\n}\n.border_tbl td,.border_tbl th{\n    border:1px black solid;\n}\n.font_c{\n    color:#5B9BD5 !important;\n}\n._c_green{\n    background:rgba(150,250,180,0.5)\n}\n",
